{
    "name": "rits/modular",
    "description": "Modular structure for laravel projects.",
    "keywords": ["framework", "laravel", "modules"],
    "license": "MIT",
    "homepage": "https://rits.com.br",
    "type": "library",
    "require": {
        "php": "^7.0",
        "laravel/framework": "~5.1"
    },
    "require-dev": {
        "pragmarx/laravelcs": "^0.1.0",
        "phpunit/phpunit": "^6.1",
        "mockery/mockery": "^0.9.9",
        "satooshi/php-coveralls": "^1.0",
        "codeclimate/php-test-reporter": "^0.4.4",
        "friendsofphp/php-cs-fixer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Rits\\": "src/Rits/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "stub/application/app/"
        },
        "files": [
            "tests/TestCase.php"
        ]
    },
    "authors": [
        {
            "name": "Lucas Ferreira",
            "email": "lucasferreira@rits.com.br"
        }
    ],
    "minimum-stability": "stable"
}
